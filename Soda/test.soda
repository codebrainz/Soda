
namespace std.math
{

    enum LowNumber
    {
        ZERO = 0,
        ONE = 1,
        TWO = 2
    }

    int fib(int n)
    {
        if (n == LowNumber.ZERO)
            return LowNumber.ZERO;
        else if (n == LowNumber.ONE)
            return LowNumber.ONE;
        else
            return (fib(n - LowNumber.ONE) + fib(n - LowNumber.TWO));
    }

}

int main()
{
    return math.fib(3);
}
